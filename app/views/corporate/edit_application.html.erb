      text-align: center;
      outline: none;
      width: 90%;
      padding: 10px;
      margin-top: 30px;
    }

    .next-btn {
      border-radius: 3px;
      padding: 10px 30px;
      font-weight: bold;
      border: none;
      font-family: Montserrat-Regular;
      background: #c3bbbb38;
      cursor: pointer;
    }

    .page-head {
      font-weight: bold;
      text-align: center;
      font-size: 17px;
    }

    .quehead {
      font-weight: bold;
      font-size: 17px;
      padding: 14px 0px;
      font-family: Poppins-Regular;
      text-align: center;
    }

    .radiobtn {
      width: 0;
      height: 35px;
      cursor: pointer;
      margin: 0;
    }

    .radiobtn::before {
      content: " ";
      background-color: white;
      border: 4px solid black;
      padding: 7px;
      left: -30px;
      top: -3px;
      position: absolute;
    }

    .radiobtn:checked::before {
      content: " ";
      border: 6px solid black;
      padding: 7px;
      background-color: white;
    }

    .optlabel {
      cursor: pointer;
      line-height: 40px;
      position: relative;
      font-family: Montserrat;
      font-size: 14px;
      margin-bottom: 12px;
    }

    /*.optlabel {*/
    /*display: block;*/
    /*position: relative;*/
    /*padding-left: 35px;*/
    /*margin-bottom: 12px;*/
    /*cursor: pointer;*/
    /*font-size: 22px;*/
    /*-webkit-user-select: none;*/
    /*-moz-user-select: none;*/
    /*-ms-user-select: none;*/
    /*user-select: none;*/
    /*}*/

    /* Hide the browser's default radio button */
    .optlabel input {
      position: absolute;
      opacity: 0;
      cursor: pointer;
    }

    /* Create a custom radio button */
    .checkmark {
      content: " ";
      background-color: white;
      border: 4px solid black;
      padding: 7px;
      left: -40px;
      top: 0px;
      position: absolute;
    }

    /* On mouse-over, add a grey background color */
    .optlabel:hover input ~ .checkmark {
      background-color: #ccc;
    }

    /* When the radio button is checked, add a blue background */
    .optlabel input:checked ~ .checkmark {
      background-color: #ffffff;
    }

    /* Create the indicator (the dot/circle - hidden when not checked) */
    .checkmark:after {
      content: "";
      position: absolute;
      display: none;
    }

    /* Show the indicator (dot/circle) when checked */
    .optlabel input:checked ~ .checkmark:after {
      display: block;
    }

    /* Style the indicator (dot/circle) */
    .optlabel .checkmark:after {
      content: " ";
      background-color: white;
      border: 6px solid black;
      padding: 7px;
      left: -6px;
      top: -4px;
      position: absolute;
    }

    option[value=""] {
      display: none;
    }

    .prompt {
      width: 96%;
      text-align-last: center;
