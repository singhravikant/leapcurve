<script>
    <%
        for i in 1..@applicant['Questions'].length do
        %>
    var opt = document.getElementsByName('que' +<%= i.to_s %>)

    opt[<%=((@applicant['Questions']['que'+i.to_s]).to_i)%>].checked = true
    <% end %>
    function upload() {
        var alert = document.getElementById('alert');
        var submit = document.getElementById('submit');
        var file = document.getElementById('file').value;
        // var index=file.lastIndexOf('.');
        var ext = file.split('.');
        if (file == '' || file == null) {
            submit.disabled = false;
        }
        else if (!(ext[ext.length - 1] == 'pdf')) {
            alert.innerHTML = 'Please Select File of PDF format';
            alert.style.display = 'block';
            submit.disabled = true;
            setTimeout(function () {
                alert.style.display = 'none'
            }, 2000);
        } else {
            submit.disabled = false;

        }
    }
    var answers=new Array();
    function nextslide(id) {
        var alert = document.getElementById('alert');
        var isnext = false;
        if (id <= 5) {
            var fields = document.getElementsByClassName('field');
            if (id == 1) {
                if (fields[0].value == null || fields[0].value == "") {
                    alert.innerHTML = 'Please Fill Name';
                    alert.style.display = 'block';
                } else if (fields[1].value == null || fields[1].value == "") {
                    alert.innerHTML = 'Please Fill Email';
                    alert.style.display = 'block';
                } else {
                    var atind = fields[1].value.indexOf('@');
                    var dotind = fields[1].value.lastIndexOf('.');
                    if (atind > 0 && dotind - atind > 2 && dotind < (fields[1].value.length - 2)) {
                        isnext = true;
                    } else {
                        alert.innerHTML = 'Please Enter Vailid Email';
                        alert.style.display = 'block';
                    }

                }
            }
            if (id == 2) {
                if (fields[3].value == null || fields[3].value == "") {
                    alert.innerHTML = 'Please Fill Highest Degree';
                    alert.style.display = 'block';
                } else if (fields[4].value == null || fields[4].value == "") {
                    alert.innerHTML = 'Please Fill Institution';
                    alert.style.display = 'block';
                } else {
                    isnext = true;
                }
            }
            if (id == 3) {
                if (fields[5].value == null || fields[5].value == "") {
                    alert.innerHTML = 'Please Fill Year Of Experience';
                    alert.style.display = 'block';
                } else if (fields[6].value == null || fields[6].value == "") {
                    alert.innerHTML = 'Please Fill Technical Skills';
                    alert.style.display = 'block';
                } else {
                    isnext = true;
                }
            }
            if (id == 4) {
                if (fields[7].value == null || fields[7].value == "") {
                    alert.innerHTML = 'Please Fill Designation';
                    alert.style.display = 'block';
                } else if (fields[8].value == null || fields[8].value == "") {
                    alert.innerHTML = 'Please Fill Company';
                    alert.style.display = 'block';
                } else if (fields[9].value == null || fields[9].value == "") {
                    alert.innerHTML = 'Please Fill Industry';
                    alert.style.display = 'block';
                } else if (fields[10].value == null || fields[10].value == "") {
                    alert.innerHTML = 'Please Fill Function';
                    alert.style.display = 'block';
                } else {
                    isnext = true;
                }
            }
            if (id == 5) {
                isnext = true;
            }

        } else {
            var questions = document.getElementsByName('que' + (id - 5));
            for (var i = 0; i < questions.length; i++) {
                if (questions[i].checked) {
                    isnext = true;
                    answers[id-6]=[i];
                    break;
                }
            }
            if (!isnext) {
                alert.innerHTML = 'Please Select Any Option';
                alert.style.display = 'block';
            }
        }
        if (isnext) {
            var slide = document.getElementById('slide' + id);
            slide.classList.remove('show');
            slide.classList.add('hide');
            var questions = document.getElementsByName('que' + (id - 4));
            id=id-4;

            if (id == 3 && (answers[0] == 10||answers[1] == 0)) {
                if(answers[0] == 10){
                    questions[5].checked = true;
                }
                if (answers[1] == 0) {
                    questions[0].checked = true;
                }

                id = id + 1;
            }
            if (id == 6 && (answers[0] == 7 || answers[0] == 8 || answers[0] == 9 || answers[0] == 10  || answers[4] == (6||7))) {
                questions[3].checked = true;
                id = id + 1;
            }
            if (id == 11 && (answers[4] == 0 || answers[4] ==1 || answers[9] ==10 ) ) {

                if (answers[4] == 0) {
                    questions[0].checked = true;
                }
                if (answers[4] ==1) {
                    questions[1].checked = true;
                }
                if (answers[9] ==10) {
                    questions[9].checked = true;
                }

                id = id + 1;
            }
            if (id == 12 && (answers[4] == 0 || answers[10] == 0)) {
                questions[0].checked = true;
                id = id + 1;
            }
            if (id == 13 && (answers[4] == 0 || answers[10] == 0 || answers[11] == 0)) {
                questions[0].checked = true;
                id = id + 1;
            }
            if (id == 16 && (answers[4] == 0 || answers[10] == 0 || answers[11] == 0)) {
                questions[0].checked = true;
                id = id + 1;
            }
            if (id == 17 && (answers[9] == 0 )) {
                questions[0].checked = true;
                id = id + 1;
            }
            if (id == 18 && (answers[0] == 8 || answers[0] == 9 || answers[0] == 10||answers[4] == (6||7))) {
                if(answers[0] == 8 ){
                    questions[4].checked = true;
                }else{
                    questions[5].checked = true;
                }
                id = id + 1;
            }
            id=id+4;


            var nextslide = document.getElementById('slide' + (id + 1));
            nextslide.classList.add('show');
            nextslide.classList.remove('hide');
        } else {
            setTimeout(function () {
                alert.style.display = 'none'
            }, 2000);
        }

    }

