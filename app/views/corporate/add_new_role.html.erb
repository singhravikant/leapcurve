    function validate() {
        var que_field = document.getElementsByName('question')[0].value;
        var type_field = document.getElementsByName('que_type');
        var option_field = document.getElementsByName('options[]');
        var options = new Array();
        var type_selected = false;

        for (var i = 0; i < option_field.length; i++) {
            var val = option_field[i].value;
            if (val != "") {
                if(!options.includes(val.toLowerCase())){
                    options[i] = val.toLowerCase();
                }else{
                    // window.alert('You have entered the same option multiple times. : '+val)
                    window.alert('You have entered the same option multiple times.')
                    return false;
                }

            }

        }
        for (var i = 0; i < type_field.length; i++) {
            var val = type_field[i].checked;
            if (val == true) {
                type_selected = true;
                break;
            }

        }
        var alert = document.getElementById('flash');
        if (que_field == "") {
            showalert("Please enter a question")
            return false;
        } else if (type_selected == false && type_field[0].type != "hidden") {
            showalert("Please select the Question type")
            return false;
        } else if (option_field.length != 0 && options.length < 2) {
            showalert("Please provide atleast two options")
            return false;
        }
        return true;
    }

    function hidealert() {
        setTimeout(function () {
            document.getElementById('flash').classList.remove('show');
        }, 5000)
    }

    function showalert(text) {
        var alert = document.getElementById('flash');
        alert.innerHTML = text;
        alert.classList.add('show');
        alert.classList.add('alert-success');

    }

