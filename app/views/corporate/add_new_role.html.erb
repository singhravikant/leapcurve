    function delete_que(id, ask) {
        if (ask == true) {
            var bool = confirm('Do you want to delete this Question?')
        } else if (ask == 'only') {
            var que = document.getElementById(id)
            document.getElementById('opt_que').removeChild(que);
            var questions = document.getElementsByClassName('questions-tab');
            // if (questions.length == 0) {
            //     document.getElementById('optional').style.display = 'none';
            // }
            bool = false;
        } else {
            bool = true;
        }
        if (bool) {
            $.ajax({
                url: '/corporate/delete_question',
                type: 'post',
                data: $('form').serialize() + "&toDelete=" + id+"&permanent="+ask,
                success: function (response) {
                    var que = document.getElementById(id)
                    document.getElementById('opt_que').removeChild(que);
                    var questions = document.getElementsByClassName('questions-tab');
                    if (questions.length == 0) {
                        document.getElementById('optional').style.display = 'none';
                    }
                }
            })
        }

    }

    function edit_que(que_id) {
        var bool = confirm('Do you want to edit this Question?')
        if (bool) {
            $.ajax({
                url: '/corporate/edit_question',
                type: 'post',
                data: $('form').serialize() + "&toEdit=" + que_id,
                success: function (response) {
                    var que_head
                    var que_data = JSON.parse(response);
                    var que_data1 = JSON.parse(que_data[0]);
                    for (var key in que_data1) {
                        if (key != "") {
                            que_head = key;
                            break;
                        }
                    }
                    var question='';
                    if ('question' in que_data1[key]){
                        question=que_data1[key]['question']
                    }else{
                        question=que_head;
                    }

                    if (que_data1[que_head]['type'] == 'text') {
                        open_tab('single');
                        var que = document.getElementsByName('question')[0].value = question;
                        // var que = document.getElementsByName('que_type')[0].selected = true;
                        for (var chip in que_data1[que_head]['keywords']) {
                            var chip_input = document.getElementsByName('keywords[]')[chip];
                            chip_input.value = que_data1[que_head]['keywords'][chip];
                            var e = $.Event("keyup", {keyCode: 13});
                            create_chip(e, 4)

                        }

                    } else if (que_data1[que_head]['type'] == 'radio' || que_data1[que_head]['type'] == 'checkbox') {
                        open_tab('multiple');
                        if (que_data1[que_head]['type'] == 'radio') {
                            open_tab('multiple');
                            var que = document.getElementsByName('question')[0].value = question;
                            var que = document.getElementsByName('que_type')[1].checked = true;
                        }
                        if (que_data1[que_head]['type'] == 'checkbox') {
                            var que = document.getElementsByName('question')[0].value = question;
                            var que = document.getElementsByName('que_type')[0].checked = true;
                        }
                        document.getElementsByName('required')[que_data1[key]['required']].checked = true
                        for (var add in que_data1[que_head]['options']) {
                            if (add < 2) {
                                var chip_input = document.getElementsByName('options[]')[add];
                                chip_input.value = que_data1[que_head]['options'][add];
                            } else {
                                addmore()
                                var chip_input = document.getElementsByName('options[]')[add];
                                chip_input.value = que_data1[que_head]['options'][add];
                            }
                        }
                    }
                    edit = que_id;
                    // edit = document.getElementsByName('question')[0].value;
                    return true;
                }
            })
        }

    }

