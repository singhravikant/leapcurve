<script>
    $(document).ready(function () {
        $("#flip").click(function () {
            $("#panel").slideToggle("slow");
        });
        $('.span-overflow').hover(function () {
            var data = $('.span-overflow');
            for (var i = 0; i < data.length; i++) {
                if (data[i] != this && isOverflown(data[i])) {
                    // data[i].style.display = "inline-block"
                } else {
                    this.style.display = "block"
                    var childs = this.children;
                    for (var j = 0; j < childs.length; j++) {
                        childs[j].style.display = "inherit"
                    }


                }
            }

        })
        $('.span-overflow').mouseout(function () {
            var data = $('.span-overflow');
            for (var i = 0; i < data.length; i++) {
                if (data[i].innerText.length > 20) {
                    data[i].style = ""
                    var childs = this.children;
                    for (var j = 0; j < childs.length; j++) {
                        childs[j].style.display = ""
                    }
                }
            }

        })

        function isOverflown(element) {
            return element.scrollHeight > element.clientHeight || element.scrollWidth > element.clientWidth;
        }
    });
</script>
