        if (file == '' || file == null) {
            alert.innerHTML = 'Please upload your CV';
            alert.style.display = 'block';
            submit.disabled = true;
            setTimeout(function () {
                alert.style.display = 'none'
            }, 2000);
            submit.disabled = false;
            return false;
        }
        else if (!(ext[ext.length - 1] == 'pdf')) {
            alert.innerHTML = 'Please select file of PDF format';
            alert.style.display = 'block';
            submit.disabled = true;
            setTimeout(function () {
                alert.style.display = 'none'
            }, 2000);
            return false;
        } else {
            submit.disabled = false;
            return true;


        }
