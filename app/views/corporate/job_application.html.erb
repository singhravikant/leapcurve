    function hideOptions(id) {
        var Options = document.getElementsByClassName('hideOptions');
        var showlessbtn = document.getElementsByClassName('showless');
        for (var i = 0; i < showlessbtn.length; i++) {
            showlessbtn[i].classList.remove("hide");
            showlessbtn[i].classList.add("hide");
            showlessbtn[i].classList.remove("show1");
        }
        for (var i = 0; i < Options.length; i++) {
            var ishide = Options[i].classList;
            for (var j = 0; j < ishide.length; j++) {
                if (ishide[j] == "hide") {
                    ishide.remove("hide");
                }
                if (ishide[j] == "show1") {
                    ishide.remove("show1");
                }

            }

            var range = Options[i].id;
            var arr = range.split(',')

            if (id < 5 && !(id >= parseInt(arr[0]) && id <= parseInt(arr[1]))) {
                Options[i].classList.add("hide");
                var siblings = Options[i].parentElement.children;
                for (var k = 0; k < siblings.length; k++) {
                    var classes = siblings[k].classList;
                    for (var l = 0; l < classes.length; l++) {
                        if (classes[l] == 'showmore') {
                            classes.remove("hide");
                            classes.add("show1");
                        }
                    }
                }


            }

        }
        if (document.getElementsByName('customers')[1].checked == true || document.getElementsByName('customers')[0].checked == true) {
            for (var j = 10; j <= 13; j++) {
                if (j != 12 && j != 13) {
                    var opt = document.getElementById('hide_show' + j).children;
                    var lst_opt = opt[opt.length - 3];
                    lst_opt.classList.add('hide')
                    var siblings = lst_opt.parentElement.children;
                    for (var k = 0; k < siblings.length; k++) {
                        var classes = siblings[k].classList;
                        for (var l = 0; l < classes.length; l++) {
                            if (classes[l] == 'showmore') {
                                classes.remove("hide");
                                classes.add("show1");
                            }
                        }
                    }
                }
                else if (j == 13) {
                    var opt = document.getElementById('hide_show' + j).children;
                    var lst_opt = opt[opt.length - 3];
                    lst_opt.classList.add('hide')
                    var sec_lst_opt = opt[opt.length - 4];
                    sec_lst_opt.classList.add('hide')
                    var siblings = lst_opt.parentElement.children;
                    for (var k = 0; k < siblings.length; k++) {
                        var classes = siblings[k].classList;
                        for (var l = 0; l < classes.length; l++) {
                            if (classes[l] == 'showmore') {
                                classes.remove("hide");
                                classes.add("show1");
                            }
                        }
                    }
                }

            }

        }
    }

    function seemore(id) {
        // document.getElementById('seeless'+id).style.display='block';
        // document.getElementById('seemore'+id).style.display='none';
        var options = document.getElementById('hide_show' + id).children;
        for (var i = 0; i < options.length; i++) {
            var classes = options[i].classList;
            for (var j = 0; j < classes.length; j++) {
                if (classes[j] == "hide") {
                    classes.add('show1');
                    classes.remove("hide");
                    break;
                } else if (classes[j] == "show1") {
                    classes.add("hide");
                    classes.remove("show1");
                    break;
                }
            }

        }
    }

