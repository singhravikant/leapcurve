<script>
    var id = 1;
    var count_chip = 1;
    if (count_chip >= 10) {
        document.getElementById('chip_input').disabled = true;
    } else {
        document.getElementById('chip_input').disabled = false;
    }

    function create_chip(e) {
        // alert(e.keyCode)
        var value = document.getElementById('chip_input').value.toUpperCase();
        if (e.keyCode == 13 && value != "") {
            var already_exist = false;
            var old_chip = $('input[value="' + value + '"]');
            if (old_chip.length != 0) {
                for (var j = 0; j < old_chip.length; j++) {
                    if (old_chip[j].readOnly == true) {
                        already_exist = true;
                        message('You have already entered this skill');
                        break;
                    }
                }
            }
        }
        if (e.keyCode == 13 && value.length > 0 && count_chip <= 10 && already_exist == false) {
            var spanadd = document.createElement("span");
            spanadd.classList.add('chip');
            spanadd.id = 'del' + id;
            spanadd.innerHTML = '<input type="value" id="chip' + id + '" name="Technical_skills[]" value="' + value + '" readonly="true" class="chip_btn" ><span class="dei_chip" onclick="delete_chip(' + id + ')">x</span>'
            document.getElementById('chips').appendChild(spanadd);
            document.getElementById('chip_input').value = "";
            document.getElementById('chip' + id).style.width = ((document.getElementById('chip' + id).value.length + 1) * 8) + 'px';
            if (count_chip >= 10) {
                document.getElementById('chip_input').disabled = true;
            } else {
                document.getElementById('chip_input').disabled = false;
            }
            id = id + 1;
            count_chip = count_chip + 1;

            return false
        } else if (e.keyCode == 13) {
            return false
        }
    }

    function delete_chip(id) {
        var chip = document.getElementById('del' + id);
        document.getElementById('chips').removeChild(chip);
        count_chip = count_chip - 1;
        if (count_chip <= 10) {
            document.getElementById('chip_input').disabled = false;
        } else {
            document.getElementById('chip_input').disabled = true;
        }
    }

