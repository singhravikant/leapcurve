<div class="comtainer_que">
  <h2 id="heading">ADD A NEW ROLE</h2><span id="alertmsg" class="alert">Select any option</span>
  <div class="head">Part 2:Role Career Level</div>
  <div class="small-txt">(<span id="percen"></span> % Completed)</div>
  <%= form_tag(:action => :getAllQuestion, :method => :post) %>
  <%= hidden_field_tag('roleid',@roleid) %>
  <div class="hide show" id="que1">

    <div>
      <h3><%= ind = 1 %> <%= @allQues[ind - 1]['queTxt'] %></h3>
      <div class="opt">
        <% @allQues[ind - 1]['queOption'].each_with_index do |key, indx| %>
          <%#= ind %>
          <% key.each do |opt, optTxt| %>

            <% if opt == 'optText' %>
              <label class="optlabel"><input type="radio" name="que<%= ind %>" value="<%= indx %>" onclick="percent(<%=ind%>)"class='radiobtn'required>
               <span><% text=optTxt.split('*') %>
                <% for i in 0..text.length-1 do %>
                  <% if i%2==0 %>
                    <%= text[i] %>
                  <% else %>
                    <b><%= text[i] %></b>
                  <% end %>
                <% end %></span>
                <br></label>
            <% end %>
          <% end %>
        <% end %>
      </div>
    </div>

    <%= link_to 'BACK','/corporate/add_role_info/',:class=>'btn'%>
    <%= button_tag "NEXT", :type => 'button',:class=>'btn', :onclick => 'next(1)' %>
  </div>

