                <% end %></span><br></label>
            <% end %>
          <% end %>
        <% end %>
      </div>
    </div>
    <%= button_tag "PREVIOUS", :type => 'button',:class=>'btn', :onclick => 'pre(15)' %>
    <%= button_tag "NEXT", :type => 'button',:class=>'btn', :onclick => 'next(15)' %>
  </div>
  <div class="hide" id="que16">

    <div>
      <h3><%= ind = 18 %> <%= @allQues[ind - 1]['queTxt'] %></h3>
      <div class="opt">
        <% @allQues[ind - 1]['queOption'].each_with_index do |key, indx| %>
          <%#= ind %>
          <% key.each do |opt, optTxt| %>

            <% if opt == 'optText' %>
              <label class="optlabel"><input type="radio" name="que<%= ind %>" value="<%= indx %>" onclick="percent(<%=ind%>)"class='radiobtn'required>
               <span><% text=optTxt.split('*') %>
                <% for i in 0..text.length-1 do %>
                  <% if i%2==0 %>
                    <%= text[i] %>
                  <% else %>
                    <b><%= text[i] %></b>
                  <% end %>
                <% end %></span><br></label>
            <% end %>
          <% end %>
        <% end %>
      </div>
    </div>
    <%= button_tag "PREVIOUS", :type => 'button',:class=>'btn', :onclick => 'pre(16)' %>
    <%= button_tag "NEXT", :type => 'button',:class=>'btn', :onclick => 'next(16)' %>

  </div>
  <div class="hide" id="que17">

    <div>
      <h3><%= ind = 19 %> <%= @allQues[ind - 1]['queTxt'] %></h3>
      <div class="opt">
        <% @allQues[ind - 1]['queOption'].each_with_index do |key, indx| %>
          <%#= ind %>
          <% key.each do |opt, optTxt| %>

            <% if opt == 'optText' %>
              <label class="optlabel"><input type="radio" name="que<%= ind %>" value="<%= indx %>" onclick="percent(<%=ind%>)"class='radiobtn'required>
               <span><% text=optTxt.split('*') %>
                <% for i in 0..text.length-1 do %>
                  <% if i%2==0 %>
                    <%= text[i] %>
                  <% else %>
                    <b><%= text[i] %></b>
                  <% end %>
                <% end %></span><br></label>
            <% end %>
          <% end %>
        <% end %>
      </div>
      <%= button_tag "PREVIOUS", :type => 'button',:class=>'btn', :onclick => 'pre(17)' %>
      <%= submit_tag 'Confirm',:class=>'confirmbtn' %>
    </div>

  </div>


</div>

<script>


    var per=document.getElementById('percen');
    var value=0;
    per.innerHTML=value;
    var old=0;
    function percent(id) {
        newvalue=value+parseInt((id*100)/19);
        if(newvalue>=old){
            per.innerHTML=newvalue;
            old=newvalue;
        }


    }
    function next(id) {

        if(id==1){
            var questions1=document.getElementsByName('que' +id);
            // var questions2=document.getElementsByName('que' +( id+1));
        }else if (id == 2) {
            var questions1 = document.getElementsByName('que' + id);
            // var questions2=document.getElementsByName('que' +( id+1));
        }
        else if (id == 3) {
            var questions1 = document.getElementsByName('que' + id );
            var questions2 = document.getElementsByName('que' + (id + 1));
        }
        else if (id == 4) {
            var questions1 = document.getElementsByName('que' + (id + 1));

        }
        else if (id == 5) {
            var questions1 = document.getElementsByName('que' + (id + 1));
            var questions2 = document.getElementsByName('que' + (id + 2));
        } else if(id>=6){
            var questions1=document.getElementsByName('que' +( id+2));
        }
        var isAnswer=false;
        for(var j=0;j<questions1.length;j++){
            if(questions1[j].checked==true){
                isAnswer=true;
                if (id == 3 || id == 5) {
                    var isAnswer = false;
                    for (var j = 0; j < questions2.length; j++) {
                        if (questions2[j].checked == true) {
                            isAnswer = true;
                            break;
                        }

                    }
                }
                break;
            }


        }

        if(isAnswer){
            document.getElementById('que' + id).classList.remove('show');
            document.getElementById('que' + (id + 1)).classList.add('show');
            document.getElementById('que' + (id + 1)).classList.remove('hide');
            document.getElementById('que' + id).classList.add('hide');
        }else
        {
            document.getElementById('alertmsg').classList.add('show');
            setTimeout(function () {
                document.getElementById('alertmsg').classList.remove('show');
            }, 2000)
        }
    }

    function pre(id) {
        document.getElementById('que' + id).classList.remove('show');
        document.getElementById('que' + (id - 1)).classList.add('show');
        document.getElementById('que' + (id - 1)).classList.remove('hide');
        document.getElementById('que' + id).classList.add('hide');
    }
</script>