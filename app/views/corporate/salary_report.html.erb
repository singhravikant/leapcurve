              <ul class="all-links">
                <span>VIEW FOR :</span>
                <% @filter = notice %>
                <% if @filter == 'Function' %>
                  <% @iactive = '' %>
                  <% @factive = 'factive' %>
                <% else %>
                  <% @iactive = 'iactive' %>
                  <% @factive = '' %>
                <% end %>
                <% if @filter.nil? && @fun %>
                  <% @filter = 'Function' %>
                  <% @iactive = '' %>
                  <% @factive = 'factive' %>
                <% elsif @filter.nil? && @indfun %>
                  <% @filter = 'Industry and Function' %>
                  <% @iactive = 'iactive' %>
                  <% @factive = '' %>
                <% end %>
                <!--                <li><a>COMPANY</a></li>-->
                <% if @indfun == true %>
                  <%# @iactive = 'iactive' %>
                  <%# @factive = '' %>
                  <%= button_tag 'INDUSTRY & FUNCTION', value: "Industry and Function", :class => 'changefilter ' + @iactive %>
                  <!--                <li><a>INDUSTRY & FUNCTION</a></li>-->
                <% end %>
                <% if @fun == true %>
                  <%# @factive = 'factive' %>
                  <%# @iactive = '' %>
                  <%= button_tag 'FUNCTION', value: "Function", :class => 'changefilter ' + @factive %>
                  <!--                <li onclick=" <%# @filter = 'Funcion' %>"><a>FUNCTION</a></li>-->
                <% end %>
                <!--                <li><a>INDUSTRY</a></li>-->
