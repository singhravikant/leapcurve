    </article>
  </section>
<% end %>
<script>
    function showdata(e, id) {
        var x = e.clientX - (e.clientX * 2) / 3;
        var y = e.clientY - e.clientY / 3;

        var mod = document.getElementById(id);
        mod.style.position = 'absolute';
        mod.style.display = 'block';
        mod.style.left = x + 'px';
        mod.style.top = y + 'px';

    }

    function clearCoor(id) {
        var mod = document.getElementById(id);
        mod.style.position = 'absolute';
        mod.style.display = 'none';
    }
</script>